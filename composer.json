{
    "name": "alexlisenkov/laravel-web-push",
    "description": "Send push notifications from your backend via the Web Push Protocol",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Lisenkov",
            "email": "alex@create.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "AlexLisenkov\\LaravelWebPush\\": "src/"
        }
    },
    "require": {
        "php": "^7.1",
        "simplito/elliptic-php": "^1.0",
        "spomky-labs/base64url": "^2.0",
        "ext-gmp": "*",
        "ext-openssl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "web-token/jwt-framework": "^2.2",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "~4.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexLisenkov\\LaravelWebPush\\LaravelWebPushServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
